)CLUSTERED BY (CIF , SUB_CLASSIFICATION_ID) INTO 16 BUCKETS
STORED AS ORC
TBLPROPERTIES	('TRANSACTIONAL'='TRUE', "ORC.COMPRESS"="SNAPPY")
;

